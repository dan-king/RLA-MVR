<div>
    <h1><i class="fa fa-upload"></i> Load Contest</h1>
    {{#if message}}
        <div class="alert alert-info">
            <h1><i class="fa fa-info-circle"></i> {{ message }}</h1>
        </div>
    {{/if }}
</div>
<code>{{upload_json_name}}</code>

<div class="alert alert-success">
    <h2><i class="fa fa-upload"></i> Success loading and creating contest!</h2>
</div>

<div class="container">
    <div class="row alert alert-success" id="jsonContest">

    </div>
    <div class="row">
        <div class="col-xs-6 alert alert-success">
            Candidates
        </div>
        <div class="col-xs-6 alert alert-success">
            Ballots
        </div>
    </div>
    <div class="row">
        <div class="col-xs-6 alert alert-success" id="jsonCandidates">
        </div>
        <div class="col-xs-6 alert alert-success" id="jsonBallots">
        </div>
    </div>
</div>

Suggested next pages:
<div>
    <a href="/mark-ballot" class="btn btn-success fa fa-check-circle"> Mark Ballot</a>
</div>
<br/>
<div>
    <a href="/list-ballots" class="btn btn-success fa fa-list-ol"> List Ballots</a>
</div>

<script>
// ===========================================================
// Initial page load
// ===========================================================
$(function(){
    var jsonContest = {{{json jsonContest}}}
    var stringContest = JSON.stringify(jsonContest)
    $("#jsonContest").append(stringContest);
    
    var jsonCandidateList = {{{json jsonCandidateList}}}
    $("#jsonCandidates").append(jsonCandidateList)

    var jsonBallotList = {{{json jsonBallots}}}
    $("#jsonBallots").append(jsonBallotList)


})
</script>
